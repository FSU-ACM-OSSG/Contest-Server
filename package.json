{
  "name": "acm-contest",
  "version": "1.0.0",
  "description": "Flask App for ACM Programming Contest",
  "main": "gulpfile.js",
  "scripts": {
    "make": "gulp && docker build -t contest-server .",
    "dev": "docker-compose -f deploy/development.yml up",
    "dev2": "docker-compose -f deploy/development.yml up -d",
    "prod": "gulp && npm run prod2",
    "prod2": "docker-compose -f deploy/docker-compose.yml up -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FSU-ACM/ACM-Contest-Server.git"
  },
  "author": "ACM at FSU",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FSU-ACM/ACM-Contest-Server/issues"
  },
  "homepage": "https://github.com/FSU-ACM/ACM-Contest-Server#readme",
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0"
  }
}
